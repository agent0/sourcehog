            fx:controller="de.agentlab.sourcehog.IndexActionPopupController">
    <content>
        <GridPane alignment="center" hgap="10" vgap="10">

            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" text="DB File"></Label>
